    </div>
  </div>
</div>

<div class="featured container no-gutter">

  <div class="row posts">
    {{#each products}}
      <div id="1" category="{{this.categoryId}}" class="item new col-md-4">
        <a href="/products/{{this.slug}}">
          <div class="featured-item">
            <img src="{{this.productImage}}" alt="" />
            <h4>{{this.id}}</h4>
            <h6>{{this.productPrice}}₫</h6>
          </div>
        </a>
      </div>
    {{/each}}
  </div>
</div>